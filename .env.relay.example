NODE_ENV=production
RELAY_HOST=0.0.0.0
RELAY_PORT=47891
# Explicit multiaddr(s) to listen on; overrides host/port if set
RELAY_WS_LISTEN=/ip4/0.0.0.0/tcp/47891/ws
# Multiaddrs to advertise to peers (comma-separated)
RELAY_ANNOUNCE=
RELAY_STATUS_INTERVAL_MS=15000
RELAY_MAX_RESERVATIONS=500
RELAY_RESERVATION_TTL_MS=7200000
LIBP2P_DEBUG=libp2p:circuit-relay:*,libp2p:websockets:*,libp2p:connection-manager:*,libp2p:upgrader:*
